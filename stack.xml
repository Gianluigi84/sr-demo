  </description>
  <author>Maintained by Ugo Cupcic</author>
  <license>GPL</license>
  <review status="unreviewed" notes=""/>
  <url>http://ros.org/wiki/shadow_robot_ethercat</url>
  <depend stack="common" /> <!-- xacro, pluginlib -->
