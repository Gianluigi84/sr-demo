	<box size="0.015 0.015 0.020" />
      </geometry>
    </collision>
  </link>

  <joint name="denso_J0" type="revolute">
    <parent link="base"/>
    <child link="link_1"/>
    <origin xyz="0 0 0.025" rpy="0 0 0" />
    <axis xyz="1 0 0" />
    <limit lower="-2.967" upper="2.967" effort="10"
	   velocity="1.0" />
    <dynamics damping="50.5"/>
  </joint>

